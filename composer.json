{
    "name": "illegal/linky",
    "description": "Linky is a free and open source URL shortener and manager.",
    "type": "library",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Vincenzo Petrucci",
            "email": "vincenzo.petrucci@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^9.19",
        "laravel/sanctum": "^3.0",
        "livewire/livewire": "^2.11"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.4",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.2",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Illegal\\Linky\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Illegal\\Linky\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Illegal\\Linky\\ServiceProvider",
                "Illegal\\Linky\\AuthServiceProvider",
                "Illegal\\Linky\\EventServiceProvider",
                "Illegal\\Linky\\RouteServiceProvider"
            ]
        }
    }
}
